package components

templ TitleH2(text string) {
	@titleH2Styles()
	<h1 class="title_h2">{ text }</h1>
}

templ titleH2Styles() {
	<style>
		.title_h2 {
			text-align: center;
			font-size: 50px;
			font-weight: bold;
			color: var(--color-black);
			margin-bottom: 40px;
			margin-top: 0;
		}
	</style>
}
